    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/css/uikit.min.css" />

    <!-- UIkit JS -->
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/js/uikit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.5.5/dist/js/uikit-icons.min.js"></script>
</head>

<body>
    <% if (usuarioLogueado != null) { %>
        <%- include('./partials/headerLogueado')%>
    <% } else { %>
        <%- include('./partials/headerDeslogueado')%>
    <% } %>
    
    <main>
        <div class="uk-section uk-section-small">
            <div class="uk-container uk-container-small">
                <section class="info-usuario uk-flex uk-flex-around uk-width-1-1">
                    <div class="uk-width-1-3 uk-flex uk-flex-center uk-flex-middle">
                        <img src="/images/foto_perfil_usuario/cande.jpeg" alt="foto de perfil" class="foto-perfil">
                    </div>
                    <div class="detalle uk-width-2-3 uk-flex uk-flex-column uk-flex-around">
                        <div class="uk-flex uk-flex-middle">
                            <h3 class="uk-margin-remove">candevinse</h3>
                            <a class="boton" href="#seguir">Seguir</a>
                        </div>
                        <div class="datosUsuario">
                            <ul class="red-usuario uk-flex uk-flex-between uk-width-1-2">
                                <li class="seguidores">100 posts</li>
                                <li class="seguidores">200 seguidores</li>
                                <li class="seguidores">300 seguidos</li>
                            </ul>
                        </div>
                    </div>
                    
               </section>

               <section class="muro uk-flex uk-flex-middle uk-flex-center">
                    <div class="uk-grid uk-child-width-1-3 uk-width-1-1 uk-margin-remove" uk-grid>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/detallePost" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/sushi.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Algo del Japon!</p>
   
                                </div>
                                                         </a>
                        </div>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/post/detail" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/ensalada.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Hoy toco algo sano!</p>
                                </div>
                                
                            </a>
                        </div>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/post/detail" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/torta.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Para chuparse los dedos
                                </div>
                                !</p>
                            </a>
                        </div>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/post/detail" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/alfajores.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Amoo</p>
              
                                </div>
                                              </a>
                        </div>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/post/detail" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/pasta.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Riquisimas!</p>
       
                                </div>
                                                     </a>
                        </div>
                        <div class="div-post uk-card uk-card-hover uk-card-body uk-flex uk-flex-column uk-flex-middle uk-flex-center ">
                            <a href="/post/detail" class="link-detalle">
                                <div class="post">
                                    <img src="/images/posts/salmon.jpeg" alt="foto de perfil" class="foto-post">
                                    <p class="uk-text-center">Ame esto!</p>
         
                                </div>
                                                   </a>
                        </div>
                    
                      
                        
                    </div>
               </section>
            </div>
        </div>
    </main>

    <%- include('./partials/footer') %> 

</body>

</html>